{
  "format_version": "1.8.0",
  "animation_controllers": {
    "controller.animation.ravager.move": {
      "states": {
        "default": {
          "parameters": [
            "query.modified_move_speed"
          ],
          "animations": [
            {
              "walk": [
                {
                  "0.0": 0.0,
                  "1.0": 1.0
                }
              ]
            },
            { "look_at_target": [] }
          ],
          "transitions": [
            { "stunned": "query.is_stunned == 1" }
          ]
        },
        "stunned": {
          "animations": [
            { "stunned": [] }
          ],
          "particle_effects": [
            {
              "effect": "stun_particles",
              "locator": "stun"
            }
          ],
          "transitions": [
            { "default": "query.is_stunned == 0" }
          ]
        }
      }
    },
    "controller.animation.ravager.head_movement": {
      "states": {
        "default": {
          "animations": [ { "idle_mouth": [] } ],
          "transitions": [
            { "roaring": "query.is_roaring == 1" },
            { "biting": "query.is_delayed_attacking == 1" }
          ]
        },
        "roaring": {
          "animations": [ { "roaring": [] } ],
          "transitions": [
            { "default": "query.is_roaring == 0" }
          ]
        },
        "biting": {
          "animations": [ { "biting": [] } ],
          "transitions": [
            { "default": "query.is_delayed_attacking == 0" }
          ]
        }
      }
    }
  }
}